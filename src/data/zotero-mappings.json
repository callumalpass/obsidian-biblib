    { "source": "websiteType", "target": "genre", "whenItemType": ["webpage", "blogPost"] },

    { "source": "subject", "target": "title", "whenItemType": ["email"] },
    { "source": "letterType", "target": "genre", "whenItemType": ["letter"] },

    { "source": "conferenceName", "target": "event-title", "whenItemType": ["conferencePaper"] },

    { "source": "meetingName", "target": "event-title", "whenItemType": ["presentation"] },
