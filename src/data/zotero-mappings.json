    { "source": "billNumber", "target": "number", "whenItemType": ["bill"] },
    { "source": "codeVolume", "target": "volume", "whenItemType": ["bill"] },
    { "source": "codePages", "target": "page", "whenItemType": ["bill"] },

    { "source": "reportNumber", "target": "number", "whenItemType": ["report"] },
