    { "source": "network", "target": "publisher", "whenItemType": ["tvBroadcast", "radioBroadcast"] },
    { "source": "episodeNumber", "target": "number", "whenItemType": ["tvBroadcast", "radioBroadcast", "podcast"] },

    { "source": "company", "target": "publisher", "whenItemType": ["computerProgram"] },
    { "source": "programmingLanguage", "target": "genre", "whenItemType": ["computerProgram"] },
    { "source": "system", "target": "medium", "whenItemType": ["computerProgram"] },
