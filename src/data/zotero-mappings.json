    { "source": "caseName", "target": "title", "whenItemType": ["case"] },
    { "source": "court", "target": "authority", "whenItemType": ["case"] },
    { "source": "docketNumber", "target": "number", "whenItemType": ["case"] },
    { "source": "reporterVolume", "target": "volume", "whenItemType": ["case"] },
    { "source": "firstPage", "target": "page", "whenItemType": ["case"] },
