{
	"name": "obsidian-biblib",
	"version": "1.7.2",
	"description": "Create literature notes and manage bibliographic references in Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:test": "npm run build && node copy-files.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"e2e": "playwright test",
		"e2e:setup": "bash e2e-setup.sh",
		"e2e:launch": "bash e2e-launch.sh"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@electron/asar": "^3.4.1",
		"@eslint/js": "^9.17.0",
		"@jest/globals": "^30.2.0",
		"@playwright/test": "^1.57.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"electron": "^39.2.7",
		"esbuild": "^0.25.2",
		"eslint": "^9.17.0",
		"globals": "^15.14.0",
		"jest": "^30.2.0",
		"obsidian": "latest",
		"ts-jest": "^29.4.5",
		"tslib": "2.4.0",
		"typescript": "^5.3.3",
		"typescript-eslint": "^8.19.1"
	},
	"dependencies": {
		"@citation-js/date": "^0.5.1",
		"@citation-js/plugin-bibtex": "^0.7.21",
		"@citation-js/plugin-doi": "^0.7.21",
		"@citation-js/plugin-isbn": "^0.4.0",
		"@citation-js/plugin-pubmed": "^0.3.0",
		"@citation-js/plugin-wikidata": "^0.7.21",
		"buffer": "^6.0.3",
		"citation-js": "^0.7.21"
	}
}
